<!--人物基础属性-->
<object>
	<!-- 基本属性 -->
	<properties>
		<property name="Type" type="byte" public="true" private="true" save="false" desc="类型: 1-TYPE_SCENE,2-TYPE_PLAYER,4-TYPE_NPC,8-TYPE_ITEM"/>
		<property name="Sex" type="byte" public="true" private="true" save="true" desc="性别 0:男性,1:女性"/>
		<property name="Photo" type="string" public="true" private="true" save="true" desc="图像,头像"/>
		<property name="Camp" type="int" public="true" private="true" save="true" desc="角色阵营 参见角色说明文本"/>
		<property name="Race" type="int" public="true" private="true" save="true" desc="角色种族 参见角色说明文本"/>
		<property name="Job"  type="int" public="true" private="true" save="true" desc="角色职业 参见角色说明文本"/>
		<property name="Level" type="int" public="true" private="true" save="true" desc="角色等级"/>
		<property name="RoleCreateTime" type="string" public="false" private="false" save="true" desc="角色创建时间"/>
		<property name="LoginTime" type="int64" public="false" private="false" save="false" desc="角色本次进入游戏的时间"/>
		<property name="EntrySceneTime" type="int64" public="false" private="false" save="false" desc="角色进入场景的时间"/>
		<property name="LeaveSceneTime" type="int64" public="false" private="false" save="true" desc="角色退出场景的时间"/>
		<property name="MacAddress" type="string" public="false" private="false" save="true" desc="Mac地址"/>
		<property name="Protected" type="int" public="false" private="false" save="false" desc="npc是保护状态, 0:撤销保护, 1:设置保护"/>
		<property name="StopAI" type="int" public="false" private="false" save="false" desc="AI停止 0:启动AI ,1:停止AI"/>
		<property name="CurTalkID" type="int" public="false" private="false" save="true" desc="当前对话编号"/>
		<property name="CurArea" type="int" public="false" private="true" save="true" desc="当前区域" />
		<property name="SceneTriggerConfig" type="string" public="false" private="true" save="true" desc="当前场景环境触发器配置名"/>
		<property name="ToolEffect" type="string" public="true" private="true" save="false" desc="玩家当前使用的生活工具特效"/>
		<property name="DisabledEquipPosList" type="int64" public="false" private="false" save="true" desc="装备栏禁用栏位标识，对应位为1则表示被禁用"/>
		<property name="SilenceTick" type="int" public="false" private="false" save="true" desc="禁言的时间"/>
		<property name="SkillEnergyList" type="string" public="true" private="true" save="true" desc="角色当前技能使用能量集合"/>
		<property name="RemoveLimit" type="int" public="false" private="false" save="false" desc="解除装备限制 0:未解除, 1:解除(测试用)"/>
		<property name="HideSkillTip" type="int" public="false" private="true" save="true" desc="是否显示技能提示 0:不隐藏, 1:隐藏"/>
		<property name="Coin_LLQD" type="int" public="false" private="true" save="true" desc="流浪群岛副本玩家持有金钱数"/>
		<property name="ActiveScene" type="string" public="false" private="false" save="true" desc="激活了的大地图传送场景"/>
		<property name="DelaySkill" type="object" public="false" private="false" save="false" desc="延迟释放的技能"/>
		<property name="DelaySkillType" type="int" public="false" private="false" save="false" desc="技能类型，0无目标点技能，1有目标点技能"/>
		<property name="DelaySkillHandType" type="int" public="false" private="false" save="false" desc="延迟释放技能左右手类型"/>
		<property name="DelaySkillX" type="float" public="false" private="false" save="false" desc="延迟释放技能目标点x"/>
		<property name="DelaySkillY" type="float" public="false" private="false" save="false" desc="延迟释放技能目标点y"/>
		<property name="DelaySkillZ" type="float" public="false" private="false" save="false" desc="延迟释放技能目标点z"/>
		<property name="DelaySkillTime" type="int" public="false" private="true" save="false" desc="技能延迟时间ms"/>
		<property name="Pre_SceneID" type="int" public="false" private="true" save="true" desc="前个场景ID"/>
		<property name="Pre_PosX" type="float" public="false" private="true" save="true" desc="前个场景X坐标"/>
		<property name="Pre_PosY" type="float" public="false" private="true" save="true" desc="前个场景Y坐标"/>
		<property name="Pre_PosZ" type="float" public="false" private="true" save="true" desc="前个场景Z坐标"/>
		<property name="MountItemId" type="string" public="false" private="true" save="true" desc="坐骑ID"/>
		<property name="WedgedTime" type="int64" public="false" private="true" save="true" desc="可以使用脱离卡死时间"/>
		<property name="EnergyLevel" type="byte" public="true" private="true" save="false" desc="已装备的能量盾的经验等级"/>
		<property name="EnergyUpgradeNum" type="byte" public="false" private="true" save="true" desc="金币强化能量盾次数限制"/>
		<property name="EnergyPreUpgradeNum" type="byte" public="false" private="true" save="true" desc="点券强化能量盾次数限制"/>
    	<property name="SpecState" type="byte" public="true" private="true" save="false" desc="特殊玩法 需要用到的一个状态"/>
		<!--藏宝图中使用,需要下线保存,所以设置在玩家这边,add by zsq-->
		<property name="IsHunterTreasure" type="byte" public="false" private="true" save="true" desc="玩家是否在寻宝状态"/>
    <property name="CheckMoniterTime" type="int64" public="false" private="false" save="false" desc="检测客户端加速时间"/>
    <property name="LastMoniterX" type="float" public="false" private="false" save="false" desc="检测上次停止位置"/>
    <property name="LastMoniterY" type="float" public="false" private="false" save="false" desc="检测上次停止位置"/>
    <property name="LastMoniterZ" type="float" public="false" private="false" save="false" desc="检测上次停止位置"/>
    <property name="LastEquipPrtId" type="string" public="false" private="false" save="true" desc="装备专精获取上次激活的bufferid"/>
	  <property name="RoleHideAdd" type="int" public="false" private="false" save="false" desc="玩家的显示和隐身"/>
    <property name="AdjustOrientClient" type="byte" public="false" private="false" save="false" desc="客户端是否需要自动目标矫正"/>
    <property name="AdjustOrient" type="byte" public="false" private="true" save="true" desc="是否自动目标矫正"/>
    <property name="AdjustTarget" type="byte" public="false" private="false" save="false" desc="矫正的目标"/>
  </properties>
	<!-- 表属性 -->
	<records>
		<record name="talk_rec" cols="2" maxrows="16" save="false" public="false" private="false" desc="玩家定时台词表(头顶说话)">
			<column type="string" desc="台词id"/>
			<column type="byte" desc="定时时长"/>
		</record>
		<!-- 场景激活记录表，按位操作 -->
		<record name="OpenMapRec" cols="1" maxrows="32" save="true" public="false" private="false" desc="场景激活记录表">
			<column type="dword" desc="场景编号(经与策划沟通, 黑金场景id最大是999, 因此最大行数设为32)"/>
		</record>
		<!-- 飞行点激活记录表，按位操作 -->
		<record name="OpenTransPointRec" cols="1" maxrows="8" save="true" public="false" private="true" desc="飞行点激活记录表，按位操作">
			<column type="dword" desc="飞行点编号"/>
		</record>
		<record name="capital_rec" cols="3" maxrows="10" save="true" public="false" private="true" desc="资产表">
			<column type="string" desc="资产名称"/>
			<column type="byte" desc="资产类型"/>
			<column type="int64" desc="资产数量"/>
		</record>
		<record name="damage_npc_rec" cols="1" maxrows="1000" save="false" public="false" private="false" desc="照成伤害的boss表">
			<column type="object" desc="被攻击的boss"/>
		</record>
		<record name="player_movie_rec" cols="2" maxrows="1000" save="false" public="false" private="false" desc="剧情是否播放过的表">
			<column type="byte" desc="剧情id"/>
			<column type="byte" desc="0:剧情未播放过；1：剧情播放过了"/>
		</record>
		<record name="QTE_OnlyOnce_Rec" cols="1" maxrows="200" save="false" public="false" private="false" desc="记录只响应一次的QTE事件">
			<column type="string" desc="事件ID"/>
		</record>
		<record name="SkillType_Lock_State_Rec" cols="1" maxrows="200" save="true" public="false" private="true" desc="记录技能快捷栏锁定状">
			<column type="string" desc="技能快捷栏ID"/>
		</record>
		<record name="SkillType_Rec" cols="1" maxrows="200" save="true" public="false" private="true" desc="记录当前使用的快捷栏及">
			<column type="DWORD" desc="技能快捷栏ID"/>
		</record>
		<record name="NormalSkill_Pos_Rec" cols="3" maxrows="1" save="true" public="fasle" private="true" desc="普通攻击技能在几个快捷栏里的位置">
			<column type="WORD" desc="第一栏的位置"/>
			<column type="WORD" desc="第二栏的位置"/>
			<column type="WORD" desc="第三栏的位置"/>
		</record>
		<record name="EquipmentSetRec" cols="3" maxrows="32" save="false" public="fasle" private="false" desc="玩家当前套装组合表">
			<column type="DWORD" desc="套装编号"/>
			<column type="string" desc="属于该套装的装备列表"/>
			<column type="string" desc="激活的属性列表"/>
		</record>
		<record name="RequstRec" cols="3" maxrows="32" save="false" public="fasle" private="false" desc="玩家当前邀请别人的信息">
			<column type="byte" desc="请求类型"/>
			<column type="widestr" desc="玩家名"/>
			<column type="int64" desc="邀请的时刻"/>
		</record>
		<record name="open_prompt_rec" cols="2" maxrows="256" save="true" public="false" private="false" desc="系统功能开放表">
			<column type="byte" desc="系统功能ID"/>
			<column type="byte" desc="是否已点击"/>
		</record>
		<record name="count_limit_rec" cols="3" maxrows="256" save="true" public="false" private="true" desc="物品使用次数限制表">
			<column type="WORD" desc="限制ID"/>
			<column type="WORD" desc="已使用次数"/>
			<column type="double" desc="道具开始新一轮统计使用次数的时间"/>
		</record>
		<record name="connect_skill_rec" cols="3" maxrows="32" save="false" public="false" private="true" desc="技能连击信息表">
			<column type="string" desc="技能ID"/>
			<column type="int64" desc="连击技能释放时间"/>
		</record>
	</records>
