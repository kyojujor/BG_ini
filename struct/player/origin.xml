<!--人物剧本相关属性定义-->
<object>
	<!-- 基本属性 -->
	<properties>
		<property name="ShowOrigin" type="int" public="true" private="true" save="true" desc=""/>
	</properties>
	<!-- 表属性 -->
	<records>
		<!-- 身份激活记录表，按位操作 -->
		<record name="Origin_Active" cols="1" maxrows="64" save="true" public="false" private="true" desc="激活身份表">
			<column type="dword" desc="身份编号位集"/>
		</record>

		<!-- 获取身份提示表，按位操作 -->
		<record name="Origin_Notify" cols="1" maxrows="64" save="true" public="false" private="true" desc="身份获得提示表">
			<column type="dword" desc="身份编号位集"/>
		</record>

		<!-- 身份获得记录表，按位操作 -->
		<record name="Origin_Completed" cols="1" maxrows="64" save="true" public="false" private="true" desc="获得身份表">
			<column type="dword" desc="身份编号位集"/>
		</record>

		<!-- 奖励获得记录表，按位操作 -->
		<record name="Origin_Prized" cols="1" maxrows="64" save="true" public="false" private="true" desc="奖励获得表">
			<column type="dword" desc="身份编号位集"/>
		</record>

		<!-- 身份进度表 -->
		<record name="Origin_Record" cols="7" maxrows="256" save="true" public="false" private="true" desc="身份进度表">
			<column type="dword" desc="身份编号"/>
			<column type="byte" desc="进度类型"/>
			<column type="string" desc="标识"/>
			<column type="byte" desc="当前数量"/>
			<column type="byte" desc="需求数量"/>
			<column type="byte" desc="进度容器下标"/>
			<column type="string" desc="文字描述"/>
		</record>
	</records>
</object>